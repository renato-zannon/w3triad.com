<!DOCTYPE html>
<html>
  <head>
    <title>W3Triad</title>
    <%= stylesheet_link_tag :all %>
    <%#= javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div id="container">
      <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, :id => "flash_#{name}" %>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>
